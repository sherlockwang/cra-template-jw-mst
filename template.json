{
  "package": {
    "scripts": {
      "start": "cross-env HTTPS=true EXTEND_ESLINT=true FORCE_COLOR=true craco start | cat",
      "build": "cross-env INLINE_RUNTIME_CHUNK=false craco build",
      "test": "cross-env craco test",
      "eject": "react-scripts eject",
      "analyze": "cross-env source-map-explorer 'build/static/js/*.js'",
      "lint": "eslint --ignore-path .gitignore . --fix",
      "prettier": "cross-env ./node_modules/.bin/prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
      "prepare": "husky install"
    },
    "dependencies": {
      "@testing-library/jest-dom": "^5.16.4",
      "@testing-library/react": "^13.3.0",
      "@testing-library/user-event": "^13.5.0",
      "babel-plugin-named-asset-import": "^0.3.7",
      "@babel/plugin-proposal-decorators": "^7.12.1",
      "@craco/craco": "^7.0.0",
      "axios": "^1.2.0",
      "cross-env": "^7.0.2",
      "mobx": "^6.3.0",
      "mobx-react": "^7.2.0",
      "mobx-state-tree": "^5.0.3",
      "react-router-dom": "^6.3.0",
      "styled-components": "^5.3.3",
      "typescript": "^4.1.2",
      "mst-middlewares": "^5.0.4"
    },
    "devDependencies": {
      "@types/node": "^18.15.11",
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.0",
      "@types/react-router-dom": "^5.1.7",
      "@types/react-syntax-highlighter": "^13.5.2",
      "@types/react-test-renderer": "^18.0.0",
      "@types/react-transition-group": "^4.4.4",
      "@types/webpack-env": "^1.13.6",
      "@typescript-eslint/eslint-plugin": "^5.59.9",
      "@typescript-eslint/parser": "^5.59.9",
      "babel-plugin-import": "^1.13.3",
      "eslint": "^8.42.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-airbnb-typescript": "^17.0.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-config-taro": "3.6.7",
      "eslint-import-resolver-typescript": "^3.5.5",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-mobx": "^0.0.9",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react": "^7.32.2",
      "eslint-plugin-react-hooks": "^4.6.0",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "prettier": "^2.2.0",
      "progress-bar-webpack-plugin": "^2.1.0",
      "source-map-explorer": "^2.5.0",
      "webpack-bundle-analyzer": "^4.1.0",
      "web-vitals": "^0.2.4",
      "remotedev": "^0.2.9",
      "http-proxy-middleware": "^1.0.6",
      "@types/http-proxy-middleware": "^0.19.3",
      "@types/jest": "^26.0.15",
      "@types/styled-components": "^5.1.15",
      "husky": "^8.0.0",
      "lint-staged": "^10.5.1"
    },
    "lint-staged": {
      "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
        "prettier --write"
      ]
    },
    "jest": {
      "moduleNameMapper": {
        "^~(.*)$": "<rootDir>/src/$1"
      },
      "transformIgnorePatterns": [
        "node_modules/(?!axios)/"
      ]
    }
  }
}
